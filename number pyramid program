for i in range(1,6):
    for j in range(5,i,-1):
        print(" ",end=" ")
    for l in range(1,i+1):
        print(l,end=" ")
    for z in range(i-1,0,-1):
        print(z,end=" ")
    print()
-----------------------------
Output:
        1 
      1 2 1
    1 2 3 2 1
  1 2 3 4 3 2 1
1 2 3 4 5 4 3 2 1
